game:GetService("StarterGui"):SetCore("SendNotification",{
	Title = "Script made by ElementsDied#7764",
	Text = "Enjoy! (if ur MyWorld fixed)",
})

local Athmos = Instance.new("Atmosphere")
local char = lp.Character
local root = char.HumanoidRootPart
local lp = game.Players.LocalPlayer

function freeze()
game:GetService("StarterGui"):SetCore("SendNotification",{
	Title = "Freeze",
	Text = "Freeze for 5 second",
})
root.Anchored = true
wait(5)
root.Anchored = false
end

function lowspeed()
game:GetService("StarterGui"):SetCore("SendNotification",{
	Title = "Low Speed",
	Text = "Low Speed for 5 second",
})
lp.Humanoid.WalkSpeed = 5
wait(5)
lp.Humanoid.WalkSpeed = 25
end

function upgradespeed()
game:GetService("StarterGui"):SetCore("SendNotification",{
	Title = "Upgrade Speed",
	Text = "Speed now 50 for 10 second",
})
lp.Humanoid.WalkSpeed = 50
wait(10)
lp.Humanoid.WalkSpeed = 25
end

function upgradepower()
game:GetService("StarterGui"):SetCore("SendNotification",{
	Title = "Jump Power",
	Text = "JumpPower changed 50 for 10 second",
})
lp.Humanoid.JumpPower = 50
wait(10)
lp.Humanoid.JumpPower = 25
end

function blind()
game:GetService("StarterGui"):SetCore("SendNotification",{
	Title = "Blind",
	Text = "Blinded for 10 second", 
})
Athmos.Haze = 100
Athmos.Density = 100
Athmos.Decay = (106, 112, 125)
wait(10)
Athmos:Destroy()
end

function jumppower()
game:GetService("StarterGui"):SetCore("SendNotification",{
	Title = "Jumpless",
	Text = "Ur Cant jump for 5 second",
})
lp.JumpPower = 1
wait(5)
lp.JumpPower = 20
end

while true do
	wait(6)
	if math.random(1,2) == 1 then lowspeed() else freeze()

	end
end

while true do
	wait(8)
	if math.random(3,4) == 3 then blind() else jumppower()

	end
end

while true do
	wait(10)
	if math.random(5,6) == 5 then upgradespeed() else upgradepower()

	end
end
